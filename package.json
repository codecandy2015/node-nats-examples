{
  "name": "node-nats-examples",
  "version": "1.0.0",
  "description": "node-nats documentation examples",
  "main": "index.js",
  "scripts": {
    "test": "tsc && ava --verbose -T 15000",
    "debugtest": "tsc && node node_modules/.bin/ava --verbose -T 6500000 -m",
    "clean": "rm -Rf build/",
    "lint": "standard './src/*.js'",
    "fmt": "standard --fix './src/*.js'"
  },
  "engines": {
    "node": ">= 13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//nats-io/node-nats-examples"
  },
  "author": "The NATS Authors",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^13.13.1",
    "ava": "^3.7.1",
    "eslint": "^6.8.0",
    "nats": "^2.0.0-24",
    "nuid": "^1.1.4",
    "standard": "^14.3.3",
    "ts-nkeys": "^1.0.16",
    "typescript": "^3.8.3"
  },
  "ava": {
    "failFast": false,
    "require": [
      "./build/helpers/ava_fix.js"
    ],
    "files": [
      "./build/**/*.js",
      "!./build/helpers/*.js"
    ]
  }
}
